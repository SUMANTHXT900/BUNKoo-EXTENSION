{
  "manifest_version": 3,
  "name": "Gitam Timetable Extractor",
  "version": "1.0",
  "description": "Extracts timetable from the G-Student page after it has been loaded.",
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "file:///*G-Student.html*", 
    "*://*/*G-Student.html*"
    
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
} 